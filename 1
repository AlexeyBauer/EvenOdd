#include<stdio.h>
int main(){

	int a;
	printf("Enter number\n");
	fflush(stdout);

	scanf("%d", &a);
	if (a%2==0){
		printf("Even number\n");

	}else {
		printf("Odd number\n");
	}

	if (a>=0 && a<=100){
		printf("Number is in the range from 0 to 100\n");

	}else{
		printf("Number is not in the range from 0 to 100\n");
		fflush(stdout);

	}

	return 0;

}
